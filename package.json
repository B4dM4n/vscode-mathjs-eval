{
  "name": "mathjs-eval",
  "displayName": "MathJS Evaluator",
  "description": "Quickly evaluate any MathJS expression",
  "version": "0.0.3",
  "license": "MIT",
  "publisher": "B4dM4n",
  "repository": {
    "type": "git",
    "url": "https://github.com/B4dM4n/vscode-mathjs-eval"
  },
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mathjs-eval.evaluate",
    "onCommand:mathjs-eval.evaluateReplace"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "mathjs-eval.evaluate",
        "title": "Evaluate"
      },
      {
        "command": "mathjs-eval.evaluateReplace",
        "title": "Evaluate and Replace"
      }
    ]
  },
  "scripts": {
    "compile": "webpack --mode none",
    "lint": "eslint src --ext ts",
    "vscode:prepublish": "webpack --mode production",
    "watch": "webpack --mode none --watch --info-verbosity verbose"
  },
  "devDependencies": {
    "@types/mathjs": "^6.0.5",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "mathjs": "^7.0.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
